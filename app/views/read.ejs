  <body>
  <header>
    <h1><a href="/manga/index">Manga Fenix</a></h1>
      <form autocomplete="off">
          <div class="buscar">
              <input type="text" placeholder="Buscar..." id="buscar">
              <button>
                  <span class="material-symbols-outlined">search</span>
              </button>
          </div>

      </form>
  </header>

    <div class="section-1">
      <div class="visual">
        <div class="img-manga">
          <img src="<%=response.img%>" alt="">
        </div>
      </div>
      <article class="info-manga">
        <div class="titulo">
          <h1><%=response.title%></h1>
          <span class="estado">en emision</span>
          <div class="viewer">         
             <span class="material-symbols-outlined">visibility
            </span>
            <span><%=response.views%></span></div>
        </div>
        <div class="info">
          <span class="auto"><%=response.author%></span>
          <span class="fecha"><%=response.followers%></span>
          <span class="tipo"> 
            <%=response.booktype%>
          </span>
        </div>

        <p class="sipnosis">
          <%=response.description%>
        </p>

        <div class="span categorias">
          <%for(let i=0;i<response.genres.length;i++){%>
          <span><%=response.genres[i]%></span>
          <%}%>
        </div>

        <div class="puntuacion">
          <span class="material-symbols-outlined">star</span>
          <span class="material-symbols-outlined">star</span>
          <span class="material-symbols-outlined">star</span>
          <span class="material-symbols-outlined">star</span>
          <span class="material-symbols-outlined">Star_Half</span>
          <span class="rate"><%=response.rate%></span>
        </div>
      </article>
    </div>
    <section class="cont">
      <h2>capitulos</h2>
      <ul class="caps">
        <%for(let i=0;i<response.chaptersList.length;i++){%>
          <li class="contCap">
            <a href="/manga/reader/<%=response.chaptersList[i].chapterId%>">
              <span class="material-symbols-outlined">
                tag
                </span>
    
       
            <span class="titleCap"><%=response.chaptersList[i].chapterTitle%></span>
         
            <span class="material-symbols-outlined">visibility
           </span>
           <span><%=response.chaptersList[i].chapterViews%></span>
           <span class="material-symbols-outlined">
            calendar_month
            </span>
          <span class="dateCap"><%=response.chaptersList[i].chapterDate%></span>   
       </div>
            </a>

      </li>
          <%}%>

      </ul>
